package day1031;

import java.util.Scanner;

public class 백준_Scanner1 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in); // Scanner 클래스 객체 생성

		int A = sc.nextInt(); // int형 정수 A 입력 -> Ex. "1"
		int B = sc.nextInt(); // int형 정수 B 입력 -> Ex. "2"

		System.out.println(A + B); // A + B 출력 -> Ex. "1 + 2 = 3"

	}

}
