package day1031;

import java.util.Scanner;

public class 백준_서기연도 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in); // Scanner 클래스 객체 생성

		int A = sc.nextInt(); // int형 정수 A(불기연도) 입력

		int B = A - 543; // 정수 B(서기연도) = 정수 A(불기연도) - 543

		System.out.println(B); // 정수 B(서기연도) 출력

	}

}
