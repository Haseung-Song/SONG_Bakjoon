package day0126_prof;

import java.util.ArrayList;

public class Test03 {
	public static void main(String[] args) {
		int[] array = new int[3];
		
		ArrayList<String> list = new ArrayList<>();
		
		list.add("10");
		list.add("20");
		list.add("30");
		list.add("40");
		System.out.println(list.size());
		
		list.remove(0);
		System.out.println("index 0 = "+list.get(0));
		System.out.println(list.size());
	}
}
