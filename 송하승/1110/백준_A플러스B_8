package day1110;

import java.util.Scanner;

public class 백준_A플러스B_8 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in); // Scanner 클래스 객체 생성

		int T = sc.nextInt(); // 반복할 테스트 횟수

		int i = 1; // i 초기화

		while (i <= T) { // T만큼 반복!!!
			int A = sc.nextInt(); // int형 정수 A 입력
			int B = sc.nextInt(); // int형 정수 B 입력

			// A + B 출력
			System.out.println("Case #" + i + ": " + A + " + " + B + " = " + (A + B));

			i++; // i를 1부터 T까지 1만큼 증가시킴.

		}

	}

}
