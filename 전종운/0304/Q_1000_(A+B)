package Backjoon_Q;

import java.util.Scanner; // 스케너를 임포트 한다.

public class Bj_문제를풀어보자 {
	
	// A는 1 B는 2 값을 입력 하여서 3 값을 출력하라.ㅋㅋ
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in); //sc라는 이름의 스케너를 생성한다.

	int A = sc.nextInt(); // 스캐너를 통해서 (정수)A값을 입력 받는다.
	int B = sc.nextInt(); // 스캐너를 통해서 (정수)B값을 입력 받는다.
	
	System.out.println(A+B); // 출력 스케너를 통해서 입력받은 정수 A B값을 더한다.
	
	sc.close();

	}

}
