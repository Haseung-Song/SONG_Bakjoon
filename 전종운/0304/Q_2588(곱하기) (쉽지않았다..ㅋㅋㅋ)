package Backjoon_Q;

import java.util.Scanner;

public class Q_2588_곱하기_입출력사칙13 {
//문제 
	//예제입력 : 472 X 385
	
	//출력 : 472*5 = 2360
	//출력 : 472*8 = 3776
	//출력 : 472*3 = 1416
	//출력 : 472*385 = 181720
	public static void main(String[] args) {
	Scanner jc = new Scanner(System.in);
		int A = jc.nextInt();
		int B = jc.nextInt();
		
		jc.close();
		
		
		System.out.println(A*(B%10)); // 1의 자리 숫자 뽑아냄
		System.out.println(A*((B%100)/10)); //10의 자리 숫자 뽑아냄
		System.out.println(A*((B%1000)/100)); // 100의 자리 숫자 뽑아냄
		System.out.println(A*B);
	
		
		

	}

}
