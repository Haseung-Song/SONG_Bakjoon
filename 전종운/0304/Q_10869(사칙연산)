package Backjoon_Q;

import java.util.Scanner;

public class Q_10869_사칙연산 {

	public static void main(String[] args) {
	Scanner hc = new Scanner(System.in);
	
	int A = hc.nextInt();
	int B = hc.nextInt();
	
	hc.close();
	System.out.println(A+B);
	System.out.println(A-B);
	System.out.println(A*B);
	System.out.println(A/B);
	System.out.println(A%B);

	}

}
